<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Event Type<small>Control panel</small></h1>
        <ol class="breadcrumb">
            <li><a href="/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Event Type</li>
        </ol>
    </section>
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                {{#each project}}
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">{{project_name}}</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                        </div>
                    </div>
                    <div class="box-body">
                        <div class="box">
                            <div class="box-header with-border">
                                <h3 class="box-title">{{event_type_name}}</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                    <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                </div>
                            </div>
                            <div class="box-body">
                                <div id="chartKepahaman" style="height:230px"></div>
                            </div><!-- /.box-body -->
                        </div><!-- /.box -->
                        <pre><code>Dictionary&lt;string, string&gt; data = new Dictionary&lt;string, string&gt;();
data["object_name"] = "{{eventType.event_type_name}}";
data["ProjectId"] = "{{id}}";
data["EventTypeId"] = "2";
socket.Emit("Action", new JSONObject(data));</code></pre>
                    </div><!-- /.box-body -->
                </div><!-- /.box -->
                {{/each}}

