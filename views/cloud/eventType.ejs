<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <article>
        <section class="content-header">
            <h1>Documentation<small>Control panel</small></h1>
            <ol class="breadcrumb">
                <li><a href="/cloud/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Event Type</li>
            </ol>
        </section>
        <div class="content container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <section class="content-max-width">
                        <section id="introduction">
                            <!-- <div class="error-page">
                                <div class="chat error-content">
                                    <div class="item">
                                        <div class="attachment">
                                            <h4>h4h4h4h4h4h4h4h4h4</h4>
                                            <h5>h5h5h5h5h5h5h5h5h5h5</h5>
                                            <p>pppppppppppppp</p>
